R, k = map(int, input().split())

S = 3.14 * R**2
Sk = k**2
z = Sk - S
if z > 0:
    x = Sk/S
    print('Площадь квадрата больше площади круга в', x, 'раз')
if z < 0:
    y = S/Sk
    print('Площадь круга больше площади квадрата в', y, 'раз')
if z == 0:
    print("Площади геометрических фигур равны")